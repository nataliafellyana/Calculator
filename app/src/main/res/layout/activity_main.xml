<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/et_edit"
        android:layout_width="450dp"
        android:layout_height="100dp"
        android:layout_marginBottom="30dp"
        android:background="@android:color/background_light"
        android:textSize="50sp" />

    <!--<EditText-->
    <!--android:id="@+id/et_edit"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:layout_below="@+id/tv_info"-->
    <!--android:enabled="false"-->
    <!--android:gravity="bottom"-->
    <!--android:lines="2"-->
    <!--android:maxLines="2"-->
    <!--android:textSize="30sp" />-->

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/et_edit"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:background="@android:color/darker_gray"
        android:text="7"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/et_edit"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button7"
        android:background="@android:color/darker_gray"
        android:text="8"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/et_edit"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button8"
        android:background="@android:color/darker_gray"
        android:text="9"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button7"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:background="@android:color/darker_gray"
        android:text="4"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button8"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button4"
        android:background="@android:color/darker_gray"
        android:text="5"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button9"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button5"
        android:background="@android:color/darker_gray"
        android:text="6"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button4"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:background="@android:color/darker_gray"
        android:text="1"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button5"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button1"
        android:background="@android:color/darker_gray"
        android:text="2"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button6"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button2"
        android:background="@android:color/darker_gray"
        android:text="3"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button1"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:background="@android:color/darker_gray"
        android:text="0"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button_dot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button3"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button0"
        android:background="@android:color/darker_gray"
        android:text="."
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button_kali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button_div"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button9"
        android:background="@android:color/holo_orange_dark"
        android:text="x"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button_min"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button_kali"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button6"
        android:background="@android:color/holo_orange_dark"
        android:text="-"
        android:textColor="@android:color/white"
        android:textSize="30dp" />

    <Button
        android:id="@+id/button_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button_min"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@id/button3"
        android:background="@android:color/holo_orange_dark"
        android:text="+"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button_eq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button_plus"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="-49dp"
        android:layout_toRightOf="@id/button_dot"
        android:background="@android:color/darker_gray"
        android:text="="
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button_div"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/et_edit"
        android:layout_alignStart="@id/button_kali"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="15dp"
        android:background="@android:color/holo_orange_dark"
        android:text="/"
        android:textColor="@android:color/white"
        android:textSize="20dp" />

    <Button
        android:id="@+id/button_del"
        android:layout_width="294dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button0"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:background="@android:color/holo_orange_dark"
        android:text="del"
        android:textColor="@android:color/white"
        android:textSize="20dp" />


</RelativeLayout>